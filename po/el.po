msgstr "Έγγραφα"

#: lutris/util/system.py:22
msgid "Downloads"
msgstr "Λήψεις"

#: lutris/util/system.py:23
msgid "Desktop"
msgstr "Επιφάνεια εργασίας"

#: lutris/util/system.py:24 lutris/util/system.py:26
msgid "Pictures"
msgstr "Εικόνες"

#: lutris/util/system.py:25
msgid "Videos"
msgstr "Βίντεο"

#: lutris/util/system.py:27
msgid "Projects"
msgstr "Έργα"


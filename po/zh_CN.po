msgstr "Winetricks"

#: lutris/runners/wine.py:497
msgid "Wine Control Panel"
msgstr "Wine 控制面板"

#: lutris/runners/wine.py:659
msgid "Select an EXE or MSI file"
msgstr "选择一个 EXE、MSI 或 LNK 文件"

#: lutris/runners/yuzu.py:14
msgid "Yuzu"
msgstr "Yuzu"

